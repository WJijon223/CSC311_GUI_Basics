    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText">
      <graphic>
         <HBox prefHeight="0.0" prefWidth="200.0">
            <children>
               <Button mnemonicParsing="false" text="Button">
                  <HBox.margin>
                     <Insets right="5.0" />
                  </HBox.margin>
               </Button>
               <Button mnemonicParsing="false" text="Button" />
            </children>
         </HBox>
      </graphic></Label>
</VBox>
